{
  "name": "notasocial-desktop",
  "version": "0.1.0",
  "description": "Electron client example JavaScript Everywhere",
  "main": "src/index.js",
  "build": {
    "appId": "com.netlify.notasocial",
    "productName": "Notasocial",
    "directories": {
      "buildResources": "resources"
    }
  },
  "scripts": {
    "start": "electron src/index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javascripteverywhere/desktop.git"
  },
  "author": "Zero",
  "license": "MIT",
  "devDependencies": {
    "electron": "6.0.0",
    "electron-builder": "21.2.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "electron-util": "0.12.1"
  }
}
